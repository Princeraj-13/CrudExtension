{
  "name": "ksextension",
  "displayName": "crudextension",
  "publisher": "KeshavSoft",
  "description": "",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "crudextension.helloWorld",
        "title": "KSHello World"
      },
      {
        "command": "KS-CreateFolder",
        "title": "KS Create Folder"
      },
      {
        "command": "KS-CreateFile",
        "title": "KS Create File"
      },
      {
        "command": "KS-ReplaceFolder",
        "title": "KS Replace Folder"
      },
      {
        "command": "KS-CopyFolders",
        "title": "KS Copy Folders"
      },
      {
        "command": "KS-CopyFoldersInRoot",
        "title": "KS Copy Folders In Root"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
